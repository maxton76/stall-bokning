{
  "name": "@equiduty/api",
  "version": "1.0.0",
  "description": "Cloud Run API Gateway for EquiDuty using Fastify",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "seed:vaccination-rules": "tsx src/scripts/seedVaccinationRules.ts",
    "migrate:vaccination-rules": "tsx src/scripts/migrateVaccinationRules.ts",
    "init:feature-toggles": "tsx scripts/initializeFeatureToggles.ts",
    "openapi:export": "tsx scripts/export-openapi.ts",
    "openapi:validate": "swagger-cli validate openapi.json",
    "openapi:lint": "spectral lint openapi.json"
  },
  "keywords": [
    "fastify",
    "api-gateway",
    "cloud-run",
    "firebase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@equiduty/shared": "^1.1.13",
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/swagger": "^9.7.0",
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/vertexai": "^1.10.0",
    "blurhash": "^2.0.5",
    "dotenv": "^17.2.3",
    "fastify": "^5.2.0",
    "firebase-admin": "^13.0.1",
    "nodemailer": "^7.0.12",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "sharp": "^0.34.5",
    "stripe": "^20.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@stoplight/spectral-cli": "^6.15.0",
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^7.0.5",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "swagger-cli": "^4.0.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
