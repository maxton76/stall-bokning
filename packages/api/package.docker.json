{
  "name": "@equiduty/api",
  "version": "1.0.0",
  "description": "Cloud Run API Gateway for EquiDuty using Fastify",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "seed:vaccination-rules": "tsx src/scripts/seedVaccinationRules.ts",
    "migrate:vaccination-rules": "tsx src/scripts/migrateVaccinationRules.ts"
  },
  "keywords": [
    "fastify",
    "api-gateway",
    "cloud-run",
    "firebase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.2.0",
    "firebase-admin": "^13.0.1",
    "nodemailer": "^7.0.12",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "stripe": "^20.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
