{
  "name": "@stall-bokning/shared",
  "version": "1.0.0",
  "description": "Shared types, utilities, and schemas for stall-bokning monorepo",
  "main": "./index.js",
  "types": "./index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./types": {
      "types": "./types/index.d.ts",
      "default": "./types/index.js"
    },
    "./types/*": {
      "types": "./types/*.d.ts",
      "default": "./types/*.js"
    },
    "./schemas": {
      "types": "./schemas/index.d.ts",
      "default": "./schemas/index.js"
    },
    "./schemas/*": {
      "types": "./schemas/*.d.ts",
      "default": "./schemas/*.js"
    },
    "./utils": {
      "types": "./utils/index.d.ts",
      "default": "./utils/index.js"
    },
    "./utils/*": {
      "types": "./utils/*.d.ts",
      "default": "./utils/*.js"
    },
    "./constants": {
      "types": "./constants/index.d.ts",
      "default": "./constants/index.js"
    },
    "./constants/*": {
      "types": "./constants/*.d.ts",
      "default": "./constants/*.js"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "dependencies": {
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.1",
    "zod": "^3.24.1"
  }
}
