{
  "name": "@stall-bokning/shared",
  "version": "1.0.2",
  "description": "Shared types, utilities, and schemas for stall-bokning monorepo",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types/index.ts",
    "./types/*": "./src/types/*.ts",
    "./schemas": "./src/schemas/index.ts",
    "./schemas/*": "./src/schemas/*.ts",
    "./utils": "./src/utils/index.ts",
    "./utils/*": "./src/utils/*.ts",
    "./constants": "./src/constants/index.ts",
    "./constants/*": "./src/constants/*.ts",
    "./lib": "./src/lib/index.ts",
    "./lib/*": "./src/lib/*.ts"
  },
  "dependencies": {
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
